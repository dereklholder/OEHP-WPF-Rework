<Window x:Class="OEHP_WPF_Rework.JsonQuery"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OEHP_WPF_Rework"
        mc:Ignorable="d"
        Title="Query Result as Json" Height="433.858" Width="300">
    <Grid>
        <TextBox x:Name="jsonText" HorizontalAlignment="Left" Height="384" Margin="10,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="272" Text="{Binding VariableHandler.queryResultJson}"/>

    </Grid>
</Window>
